docker.enabled = true
params.igenomes_base = 's3://ngi-igenomes/igenomes'

profiles {
    test_data {
        includeConfig 'conf/test_data.config'

        params.fasta = params.test_data['sarscov2']['genome']['genome_fasta']
        params.gtf = params.test_data['sarscov2']['genome']['genome_gtf']
    }
}
